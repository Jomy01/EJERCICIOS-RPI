using System;
					
public class Program
{
	public static void Main()
	{
		int numero1;
		int numero2;
		int producto;
	
		
		do
		{
		Console.WriteLine("Introduce un número menor que 25:");
		numero1 = Convert.ToInt32(Console.ReadLine());
		}
		while (numero1 > 25);
		
		
		do
		{
		Console.WriteLine("Introduce un número mayor que 25: ");
		numero2 = Convert.ToInt32(Console.ReadLine());
		}
		while (numero2< 25);
		
		producto = numero1 * numero2;
		
		Console.WriteLine("El producto de los números introducidos es:" + producto);
	}
}
