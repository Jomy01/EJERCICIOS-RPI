using System;
					
public class Program
{
	public static void Main()
	{
		int numero1;
		int numero2;
		int producto;
		string numerointroducido1;
		string numerointroducido2;
		
		Console.WriteLine("Introduce el primer número:");
		numerointroducido1 = Console.ReadLine();
		Console.WriteLine("Introduce el segundo número:");
		numerointroducido2 = Console.ReadLine();
		
		numero1 = Convert.ToInt32(numerointroducido1);
		numero2 = Convert.ToInt32(numerointroducido2);
		
		producto = numero1 * numero2;
		
		Console.WriteLine("El producto de los números introducidos es:" + producto);
	}
}
